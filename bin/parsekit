#!/bin/bash

dir=$(d=$(dirname "$0"); cd "$d" && pwd)

if hash parser3 2>/dev/null; then
    path="parser3"
elif hash parser3.cgi 2>/dev/null; then
    path="parser3.cgi"
else
    path="${dir}/../parser/parser3.cgi"
fi

CGI_PARSER_CONFIG="${dir}/../parser/auto.p" bash -c "${path} \"${dir}/parsekit.p\" $*"
