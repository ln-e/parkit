#!/bin/bash

DIR=`dirname "$0"`
FILE_NAME="parser3 parser3.cgi"
PATH="`pwd`/cgi `pwd`/../cgi"
PARSER=

for file in $FILE_NAME; do
    for dir in $PATH; do
	if [ -f "$dir/$file" ]; then
	    PARSER="$dir/$file"
	fi;
    done;
done;

if [ -z "$PARSER" ]; then
    echo "Parser not found :("
    exit 1
fi

bash -c "$PARSER \"$DIR/parsekit.p\" $*"
